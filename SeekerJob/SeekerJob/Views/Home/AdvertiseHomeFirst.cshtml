
@using SeekerJob.Controllers
<script>
    var domain = '@Request.Url.Scheme://@Request.Url.Host:@Request.Url.Port';
</script>

@{
    var listadv = ViewData["listadv"] as List<tablebannerpart>;
    var part = 1;
}



<div class="container" style="background-color: #f5f5f5; margin-top:50px; margin-bottom:50px">
    <div class="carousel-container">
        <button class="prev-btn" onclick="prevSlide()">&#10094;</button>
        <div class="carousel-new">
            <div class="carousel-track">
                @if (listadv != null && listadv.Count > 0)
                {
                foreach (var item in listadv)
                {
                <a href="/3452345" style="width: 240px; height: 170px; display: inline-block;">
                    <img style="width:240px; height:170px" src="~/ContentImage/images/@item.link" alt="Image @part">
                </a>

                part++;
                }
                }
            </div>
        </div>
        <button class="next-btn" onclick="nextSlide()">&#10095;</button>
    </div>
</div>