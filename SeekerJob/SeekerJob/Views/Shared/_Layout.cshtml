    .custom-search-wrapper {
        width: 100%;
        max-width: 1200px;
        margin: 0 auto;
    }

    .custom-search-bar {
        display: flex;
        background-color: white;
        border-radius: 30px;
        padding: 10px;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        align-items: center;
    }

    .custom-search-input {
        flex-grow: 2;
        padding: 10px 15px;
        border: none;
        border-radius: 30px;
        font-size: 16px;
        outline: none;
    }

    .custom-dropdown-container {
        display: flex;
        gap: 10px;
        margin-left: 15px;
    }

    .custom-dropdown {
        padding: 10px;
        border: none;
        border-radius: 30px;
        background-color: #f5f5f5;
        font-size: 14px;
    }

    .custom-search-button {
        padding: 10px 20px;
        margin-left: 15px;
        background-color: #00b14f;
        border: none;
        border-radius: 30px;
        color: white;
        cursor: pointer;
        font-size: 16px;
        transition: background-color 0.3s ease;
    }

        .custom-search-button:hover {
            background-color: #008c3a;
        }

    .carousel-container {
        position: relative;
        width: 100%;
        margin: auto;
        overflow: hidden;
    }

    .carousel-new {
        display: flex;
        justify-content: space-between;
        overflow: hidden;
    }

    .carousel-track {
        display: flex;
        transition: transform 0.5s ease-in-out;
    }

    .carousel-new img {
        width: 33.33%;
        /* 1/3 of the carousel */
        padding: 5px;
        border-radius: 10px;
    }

    .prev-btn,
    .next-btn {
        position: absolute;
        top: 50%;
        transform: translateY(-50%);
        background-color: rgba(0, 0, 0, 0.5);
        color: white;
        border: none;
        font-size: 24px;
        cursor: pointer;
        padding: 10px;
        z-index: 1;
    }

    .prev-btn {
        left: 0;
    }

    .next-btn {
        right: 0;
    }
    /* CSS Tùy Chỉnh cho Carousel */

    .carousel-wrapper {
        height: 100%;
        min-height: 300px;
        /* Chiều cao tối thiểu */
    }

    .carousel-inner,
    .carousel-item {
        height: 100%;
    }

    .carousel-inner {
        max-height: 700px;
        /* Điều chỉnh chiều cao nếu cần */
        width: auto;
        object-fit: cover;
    }

    .carousel-item img {
        height: 100%;
        width: 100%;
    }
    /* Responsive Height */

    .edit_h {
        font-size: 14px;
    }

    .edit_p {
        font-size: 13px;
    }

    /* Optional: Điều chỉnh các nút điều khiển nếu cần */

    .carousel-control-prev-icon,
    .carousel-control-next-icon {
        background-color: rgba(0, 0, 0, 0.5);
        border-radius: 50%;
    }

