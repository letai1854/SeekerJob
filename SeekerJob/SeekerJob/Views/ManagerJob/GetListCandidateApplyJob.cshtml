@{
    var table = ViewData["applyjobcandidate"] as List<SeekerJob.DTO.ListCandidateApplyJob>;
}

<div class="modal fade twm-sign-up" id="sign_up_popup2" aria-hidden="true" aria-labelledby="sign_up_popupLabel2" tabindex="-1">
    <div style=" max-width:800px" class="modal-dialog modal-dialog-centered">
        <div class="modal-content">

            <div class="modal-header">
                <h2 class="modal-title" id="sign_up_popupLabel">Danh sách ứng viên nộp đơn</h2>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>

            <div class="modal-body">
                <div class="twm-tabs-style-2">
                    <div class="section-full p-t120  p-b90 site-bg-white">
                        <!--Filter Short By-->
                        <div class="twm-right-section-panel candidate-save-job site-bg-gray">
                            <div class="twm-D_table table-responsive">
                                <table style=" width: 100%" id="jobs_bookmark_table_job_candidate" class="table table-bordered twm-candidate-save-job-list-wrap">
                                    <thead>
                                        <tr>
                                            <th>Ứng viên</th>
                                            <th>Đơn ứng tuyển</th>
                                            <th>Ngày ứng tuyển</th>
                                            <th>Hàng động</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <!--1-->
                                        @foreach (var item in table)
                                        {
                                           <tr>
                                               <td>
                                                   <div class="twm-candidate-save-job-list">
                                                       <div class="twm-media">
                                                           <div class="twm-media-pic">
                                                               <img src="~/ContentImage/images/@item.image" alt="#">
                                                           </div>
                                                       </div>
                                                       <div class="twm-mid-content">
                                                           <a class="twm-nav-sign-up" data-bs-toggle="modal" href="#popup_account" role="button">
                                                               <h4>@item.name</h4>
                                                           </a>

                                                       </div>

                                                   </div>
                                               </td>
                                               <td><a href="javascript:;">@item.filecandidate</a></td>
                                               <td>
                                                   <div>@item.datesend</div>
                                               </td>

                                               <td>
                                                   <div class="twm-table-controls">
                                                       <ul class="twm-DT-controls-icon list-unstyled">
                                                           <li>
                                                               <a data-bs-toggle="modal" href="#saved-jobs-view" role="button" class="custom-toltip">
                                                                   <span class="fa fa-eye"></span>
                                                                   <span class="custom-toltip-block">Veiw</span>
                                                               </a>
                                                           </li>
                                                           <li>
                                                               <button title="Delete" data-bs-toggle="tooltip" data-bs-placement="top">
                                                                   <span class="far fa-trash-alt"></span>
                                                               </button>
                                                           </li>
                                                       </ul>
                                                   </div>
                                               </td>
                                            </tr>
                                                   }
<tr>
                                            
                                    </tbody>

                                </table>
                            </div>
                        </div>




                    </div>
                </div>
            </div>
        </div>
    </div>
</div>